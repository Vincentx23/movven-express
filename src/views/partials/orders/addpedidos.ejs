 
<div class="">
  <div>
    <!-- filtros-->
    <%- include ('filters') %>

  </div>
</div>


        <!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Fomulario de ingreso de pedidos</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form id="form-order">
          <div class="form-row">
            <div class="" id="alert1"></div>
            <div class="form-group col-md-6">
              <label for="codeDelivery">Code. Delivery</label>
              <input type="text" class="form-control" id="codeDelivery" placeholder="format:123456798">
            </div>
            <div class="form-group col-md-6">
              <label for="inputPassword4">Nombre del cliente</label>
              <input type="text" class="form-control" id="clientName"placeholder="Ejemplo: Carlos Hills">
            </div>
   
          <div class="form-group col-md-6">
            <label for="inputAddress">Contacto de teléfono.</label>
            <input type="text" class="form-control" id="phone" placeholder="Formato:6369-6584">
          </div>
          <div class="form-group col-md-6">
            <label for="inputAddress2"> Cantidad de paquetes</label>
            <input type="number" class="form-control" id="amountPakages">
          </div>

            <div class="form-group col-md-12">
              <label for="inputCity"> Dimensiones totales</label>
              <input type="text" class="form-control" id="totalDimensions"placeholder="">               
             </div>
         
            <div class="form-group col-md-6">
              <label for="inputZip">Ciudad</label>
              <input type="text" class="form-control" id="city">
            </div>
      
            <div class="form-group col-md-6">
              <label for="inputZip">Distrito</label>
              <input type="text" class="form-control" id="distric">
            </div>
            <div class="form-group col-md-12">
              <label for="inputZip">  Otros detalles de la dirección.</label>
              <input type="text" class="form-control" id="directionDetails">
            </div>         

          <div class="form-group col-md-12">
            <label for="inputZip"> Fecha límite para entrega.</label>
            <input type="text" class="form-control" id="limitDate"placeholder="Formato: dd/mm/yy">
          </div>
          <div class="form-group col-md-12">
            <label for="inputZip"> Pago total de la entrega.</label>
            <input type="text" class="form-control" id="payment"placeholder="Formato: 10.00">
          </div>
        </div>
          <div class=" form-group col-mb-12">
            <label for="orderDescription">Descripción del pedido realizado</label>
            <textarea class="form-control " id="orderDescription" placeholder="Ingrese Descripción" required></textarea>
          </div>        
        </form>        
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <button type="button" id="btn-newOrder" class="btn btn-primary">Crear</button>
      </div>
    </div>
  </div>
</div>
